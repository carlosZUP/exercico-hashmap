package br.com.example.service;

import java.util.HashMap;
import java.util.Map;

import org.springframework.stereotype.Service;

import br.com.example.postagem.PostagemModel;

@Service
public class PostagemService {
	
	private Map<Integer, PostagemModel> postagem = new HashMap<Integer, PostagemModel>();
	
	public void adicionarPostagem() {
		postagem.put(1, new PostagemModel());
		
	}
}
